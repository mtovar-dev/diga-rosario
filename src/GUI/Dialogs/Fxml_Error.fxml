<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="ap_root1" prefHeight="100.0" prefWidth="450.0" styleClass="Pane, bordered-Pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="GUI.Dialogs.Fxml_ErrorController">
  <children>
    <HBox alignment="CENTER_LEFT" layoutX="30.0" prefHeight="100.0" prefWidth="400.0" spacing="5.0">
      <children>
        <ImageView fx:id="im_icon" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../../Images/img15.png" />
          </image>
        </ImageView>
        <Label fx:id="lb_error" prefWidth="350.0" styleClass="lb_error" text="Label" wrapText="true">
          <stylesheets>
            <URL value="@Dialog.css" />
          </stylesheets>
        </Label>
      </children>
    </HBox>
    <VBox alignment="TOP_RIGHT" layoutX="400.0" prefHeight="100.0" prefWidth="50.0" spacing="5.0">
      <children>
        <ImageView fx:id="im_close" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image preserveRatio="false" smooth="false" url="@../../Images/close_button.png" />
          </image>
        </ImageView>
      </children>
      <padding>
        <Insets right="5.0" top="10.0" />
      </padding>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@Dialog.css" />
    <URL value="@../Css.css" />
  </stylesheets>
</AnchorPane>
