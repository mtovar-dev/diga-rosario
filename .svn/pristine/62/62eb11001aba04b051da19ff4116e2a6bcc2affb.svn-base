package Objects.System;

/**
 *
 * @author mtovar
 */
public class Email {
    private String usr_email;
    private String pwd;
    private String usr_to;
    private String usr_cc;
    private String usr_co;
    private String subject;
    private String message;
    private String pathFile;
    private String attachFile;

    /**
     * @return the usr_email
     */
    public String getUsr_email() {
        return usr_email;
    }

    /**
     * @param usr_email the usr_email to set
     */
    public void setUsr_email(String usr_email) {
        this.usr_email = usr_email;
    }

    /**
     * @return the pwd
     */
    public String getPwd() {
        return pwd;
    }

    /**
     * @param pwd the pwd to set
     */
    public void setPwd(String pwd) {
        this.pwd = pwd;
    }

    /**
     * @return the usr_to
     */
    public String getUsr_to() {
        return usr_to;
    }

    /**
     * @param usr_to the usr_to to set
     */
    public void setUsr_to(String usr_to) {
        this.usr_to = usr_to;
    }

    /**
     * @return the usr_cc
     */
    public String getUsr_cc() {
        return usr_cc;
    }

    /**
     * @param usr_cc the usr_cc to set
     */
    public void setUsr_cc(String usr_cc) {
        this.usr_cc = usr_cc;
    }

    /**
     * @return the usr_co
     */
    public String getUsr_co() {
        return usr_co;
    }

    /**
     * @param usr_co the usr_co to set
     */
    public void setUsr_co(String usr_co) {
        this.usr_co = usr_co;
    }

    /**
     * @return the subject
     */
    public String getSubject() {
        return subject;
    }

    /**
     * @param subject the subject to set
     */
    public void setSubject(String subject) {
        this.subject = subject;
    }

    /**
     * @return the message
     */
    public String getMessage() {
        return message;
    }

    /**
     * @param message the message to set
     */
    public void setMessage(String message) {
        this.message = message;
    }

    /**
     * @return the pathFile
     */
    public String getPathFile() {
        return pathFile;
    }

    /**
     * @param pathFile the pathFile to set
     */
    public void setPathFile(String pathFile) {
        this.pathFile = pathFile;
    }

    /**
     * @return the attachFile
     */
    public String getAttachFile() {
        return attachFile;
    }

    /**
     * @param attachFile the attachFile to set
     */
    public void setAttachFile(String attachFile) {
        this.attachFile = attachFile;
    }

    
}
