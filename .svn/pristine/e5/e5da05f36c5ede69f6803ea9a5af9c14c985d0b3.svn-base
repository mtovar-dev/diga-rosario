.lb-error{
    -fx-font-size: 16px;
}

.lb-mensj{
    -fx-font: bold 18px "Arial Black";
    -fx-text-fill:#2F88ae;
    -fx-fill: #61a2b1;
    -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 3 );    
}

.tab-pane .tab-header-area .tab-header-background {
    -fx-background-color: transparent;
}

 .prueba-Pane{
    -fx-background-color: #333333,linear-gradient(#f3f3f3 0%, #ced3da 100%);    
}

